utton>\r\n      </template>\r\n    </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {getUsers} from '@/apis/user.js'\r\n\r\nexport default {\r\n    name: 'UserListView',\r\n    data(){\r\n        return {\r\n            userList: [],\r\n        }\r\n    },\r\n    mounted(){\r\n        let that = this\r\n        getUsers().then(response =>{\r\n            that.userList = response.data.data\r\n        })\r\n    },\r\n     methods: {\r\n      handleEdit(index, row) {\r\n        alert(\"编辑\")\r\n        console.log(index, row);\r\n      },\r\n      handleDelete(index, row) {\r\n        alert(\"删除\")\r\n        console.log(index, row);\r\n      },\r\n      isBanIt(status){\r\n        if(status == \"正常\") return \"封禁此号\"\r\n        return \"解封此号\"\r\n      },\r\n      isKick(isOnline){\r\n        if(isOnline == \"在线\") return \"强制下线\"\r\n        return \"邀请上线\"\r\n      },\r\n      getBanType(status){\r\n        if(status == \"正常\") return \"primary\"\r\n        return \"danger\"\r\n      },\r\n      getKickType(isOnline){\r\n         if(isOnline == \"在线\") return \"warning\"\r\n         return \"success\"\r\n      }\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n/*最外层透明*/\r\n::v-deep .el-table,\r\n::v-deep .el-table__expanded-cell {\r\n  background-color: transparent !important;\r\n}\r\n/* 表格内背景颜色 */\r\n::v-deep .el-table th,\r\n::v-deep .el-table tr,\r\n::v-deep .el-table td {\r\n  background-color: transparent !important;\r\n}\r\n\r\n    \r\n\r\n</style>"],"sourceRoot":"src/components/manage"}]}