rom 'chart.js/auto'\r\n\r\nexport default {\r\n  name: 'UserDataView',\r\n  data() {\r\n    return {\r\n      chart1Data: {\r\n        labels: ['普通用户', '管理员', '站长'],\r\n        datasets: [\r\n          {\r\n            label: 'My First Dataset',\r\n            data: [10, 3, 1],\r\n            backgroundColor: ['rgb(255, 99, 132)', 'rgb(54, 162, 235)', 'rgb(255, 205, 86)'],\r\n            hoverOffset: 4,\r\n          },\r\n        ],\r\n      },\r\n      chart2Data: {\r\n        labels: ['12岁以下', '12-18岁', '18-22岁', '22-35岁', '35岁以上'],\r\n        datasets: [\r\n          {\r\n            label: '用户年龄分布',\r\n            data: [11, 16, 7, 3, 14],\r\n            backgroundColor: [\r\n              'rgb(255, 99, 132)',\r\n              'rgb(75, 192, 192)',\r\n              'rgb(255, 205, 86)',\r\n              'rgb(201, 203, 207)',\r\n              'rgb(54, 162, 235)',\r\n            ],\r\n          },\r\n        ],\r\n      },\r\n\r\n      chart3Data: {\r\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July',   'August',\r\n          'September',\r\n          'October',\r\n          'November',\r\n          'December',],\r\n        datasets: [\r\n          {\r\n            label: '2021年注册用户统计',\r\n            data: [65, 59, 80, 81, 56, 55, 40, 65, 49, 70, 81, 56, 45, 32],\r\n            backgroundColor: [\r\n              'rgba(255, 99, 132, 0.7)',\r\n              'rgba(255, 159, 64, 0.7)',\r\n              'rgba(255, 205, 86, 0.7)',\r\n              'rgba(75, 192, 192, 0.7)',\r\n              'rgba(54, 162, 235, 0.7)',\r\n              'rgba(153, 102, 255, 0.7)',\r\n              'rgba(201, 203, 207, 0.7)',\r\n               'rgba(255, 159, 64, 0.7)',\r\n              'rgba(255, 205, 86, 0.7)',\r\n              'rgba(75, 192, 192, 0.7)',\r\n              'rgba(153, 102, 255, 0.7)',\r\n              'rgba(201, 203, 207, 0.7)',\r\n            ],\r\n            borderColor: [\r\n              'rgb(255, 99, 132)',\r\n              'rgb(255, 159, 64)',\r\n              'rgb(255, 205, 86)',\r\n              'rgb(75, 192, 192)',\r\n              'rgb(54, 162, 235)',\r\n              'rgb(153, 102, 255)',\r\n              'rgb(201, 203, 207)',\r\n              'rgb(255, 159, 64 )',\r\n              'rgb(255, 205, 86)',\r\n              'rgb(75, 192, 192)',\r\n              'rgb(153, 102, 255)',\r\n              'rgb(201, 203, 207)',\r\n            ],\r\n            borderWidth: 1,\r\n          },\r\n        ],\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    Chart.defaults.color = 'black'\r\n    var ctx1 = document.getElementById('myChart1').getContext('2d')\r\n    var ctx2 = document.getElementById('myChart2').getContext('2d')\r\n    var ctx3 = document.getElementById('myChart3').getContext('2d')\r\n    new Chart(ctx1, {\r\n      type: 'pie',\r\n      data: this.chart1Data,\r\n    })\r\n    new Chart(ctx2, {\r\n      type: 'polarArea',\r\n      data: this.chart2Data,\r\n    })\r\n    new Chart(ctx3, {\r\n      type: 'bar',\r\n      data: this.chart3Data,\r\n      options: {\r\n      \r\n      }\r\n    })\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\nh1{\r\n    text-align: center;\r\n    font-size: 25px;\r\n    padding: 0;\r\n    height: 40px;\r\n    margin: 30px auto;\r\n    margin-bottom: 20px;\r\n    background-image: linear-gradient(135deg,#e4002b, rgb(0, 195, 255));\r\n            background-clip:text;\r\n            -webkit-background-clip:text;\r\n            color: transparent;\r\n}\r\n\r\n.topCon {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.container {\r\n  display: flex;\r\n  align-content: flex-start;\r\n  width: 49%;\r\n  height: 800px;\r\n  margin-top: 10px;\r\n  flex-wrap: wrap;\r\n\r\n}\r\n\r\n.con {\r\n  width: 100%;\r\n  height: 50%;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/manage"}]}