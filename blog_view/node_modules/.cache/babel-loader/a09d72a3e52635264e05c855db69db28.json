KAAA,IAAA,CAAA,QAAA;AACA,WAAA,iBAAA;AACA;AACA,GAxBA;;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,YAAA,CAAA,GAAA,EAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,OAAA;AACA,KAHA;;AAKA,IAAA,YAAA,GAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AACA,WAAA,MAAA,CACA,QADA,CACA,QADA,EACA,KAAA,IADA,EAEA,IAFA,CAEA,MAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA,OALA,EAMA,KANA,CAMA,KAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA;AACA,OARA;AASA,KAjBA;;AAmBA,IAAA,MAAA,CAAA,IAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KAvBA;;AAyBA,IAAA,WAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KA3BA;;AA6BA,IAAA,UAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KA/BA;;AAiCA,IAAA,WAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAnCA;;AAqCA,IAAA,aAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAvCA;;AAyCA,IAAA,QAAA,CAAA,MAAA,EAAA;AACA,WAAA,KAAA,CAAA,MAAA,EAAA,MAAA;AACA,KA3CA;;AA+CA,IAAA,iBAAA,GAAA;AACA,UAAA,KAAA,iBAAA,IAAA,6BAAA,EAAA;AACA,aAAA,iBAAA,GAAA,8BAAA;AACA,OAFA,MAEA;AACA,aAAA,iBAAA,GAAA,6BAAA;AACA;AACA;;AArDA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"nav\">\r\n    <div class=\"user\" @mouseover=\"showMenu(true)\">\r\n      <el-button style=\"margin: 10px auto\" @click=\"jumpToLogin\">\r\n        {{ userMessage }}\r\n      </el-button>\r\n      <!-- <el-button :style=\"logoutButtonStyle\" @click=\"jumpToLogout\"> 退出 </el-button> -->\r\n    </div>\r\n    <div class=\"menuContainer\">\r\n      <el-menu\r\n        :default-active=\"activeIndex2\"\r\n       \r\n        class=\"el-menu-nav\"\r\n        mode=\"horizontal\"\r\n        @select=\"handleSelect\"\r\n        background-color=\"#b8ddf7\"\r\n        text-color=\"black\"\r\n        active-text-color=\"#ffd04b\"\r\n      >\r\n        <el-menu-item @click=\"jumpTo('/home')\" >首页</el-menu-item>\r\n        <el-menu-item  @click=\"jumpTo('/home/blogclassify')\">博客分类</el-menu-item>\r\n        <el-menu-item index=\"4\"  @click=\"jumpTo('/blogwrite')\">写博客</el-menu-item>\r\n        <el-menu-item index=\"5\"  @click=\"jumpTo('/manage')\">后台管理</el-menu-item>\r\n        <el-menu-item index=\"6\" @click=\"jumpTo('/home/about')\">关于博主</el-menu-item>\r\n      </el-menu>\r\n    </div>\r\n\r\n    <div class=\"searchBar\">\r\n      <search></search>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getUserData } from '@/request/token.js'\r\nimport Search from '@/components/tool/Search.vue'\r\n\r\n\r\nexport default {\r\n  name: 'Navbar',\r\n  data() {\r\n    return {\r\n      searchInput: '',\r\n      activeIndex2: '1',\r\n      userMessage: '登录/注册',\r\n      logoutButtonStyle: 'margin: 10px; display: none',\r\n      user: {},\r\n    }\r\n  },\r\n\r\n  components:{\r\n    Search,\r\n  },\r\n\r\n  mounted() {\r\n    if (getUserData() != null) {\r\n      console.log(JSON.parse(getUserData()))\r\n      this.user = JSON.parse(getUserData())\r\n      this.$store.commit('SET_USER', this.user)\r\n      this.userMessage = this.user.nickname\r\n      this.changeLogoutStyle()\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    handleSelect(key, keyPath) {\r\n      console.log(key, keyPath)\r\n    },\r\n\r\n    jumpToLogout() {\r\n      let that = this\r\n      console.log(this.user)\r\n      this.$store\r\n        .dispatch('logout', this.user)\r\n        .then(() => {\r\n          alert('退出成功')\r\n          that.$router.go(0)\r\n        })\r\n        .catch((error) => {\r\n          alert(error)\r\n        })\r\n    },\r\n\r\n    jumpTo(path){\r\n      this.$router.push({\r\n        path: path,\r\n      })\r\n    },\r\n\r\n    jumpToLogin() {\r\n      this.$router.push({ path: '/login' })\r\n    },\r\n\r\n    jumpToHome(){\r\n      this.$router.push({ path: '/' })\r\n    },\r\n\r\n     jumpToWrite(){\r\n      this.$router.push({ path: '/blogwrite' })\r\n    },\r\n\r\n     jumpToArticle(){\r\n      this.$router.push({ path: '/blogview' })\r\n    },\r\n\r\n    showMenu(isShow){\r\n      this.$emit('show', isShow)\r\n    },\r\n    \r\n\r\n\r\n    changeLogoutStyle() {\r\n      if (this.logoutButtonStyle == 'margin: 10px; display: none') {\r\n        this.logoutButtonStyle = 'margin: 10px; display: block'\r\n      } else {\r\n        this.logoutButtonStyle = 'margin: 10px; display: none'\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.nav {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #b8ddf7;\r\n  /* opacity: 90%; */\r\n}\r\n\r\n.nav .el-menu-item {\r\n  font-size: 25px;\r\n}\r\n\r\n.nav .user {\r\n  float: left;\r\n  width: 11%;\r\n}\r\n\r\n.nav .menuContainer {\r\n  float: left;\r\n  margin: auto;\r\n}\r\n\r\n.con{\r\n  margin: 10px auto;\r\n}\r\n\r\n.nav .con .el-input {\r\n  float: left;\r\n  border-radius: 0px;\r\n  width: 200px;\r\n  height: 40px;\r\n  font-size: 20px;\r\n}\r\n\r\n.nav .con .el-button{\r\n  border-radius: 0px;\r\n  float: left;\r\n}\r\n\r\n.nav .con .el-button :hover{\r\n  background: #c4dff6;\r\n}\r\n\r\n.nav .el-submenu .el-menu-item{\r\n  z-index: 1;\r\n}\r\n\r\n.searchBar{\r\n  float: right;\r\n  height: 100%;\r\n  width: 29%;\r\n}\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}